	"description": "API for the Time Capsule bachelor project.",
	"main": "index.js",
	"scripts": {
		"prepare": "npm run gcp-build",
		"test": "jest",
		"dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node --files src/index.ts\"",
		"lint": "eslint ./src --ext .ts",
		"lint-and-fix": "eslint ./src --ext .ts --fix",
